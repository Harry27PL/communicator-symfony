{% for contact in contacts %}
    <li class="{% if interlocutor == contact %}active{% endif %}" data-id="{{ contact.id }}">

        <a href="{{ path('chat', {username: contact.username}) }}">

            <span class="fa fa-circle"></span>
            {{ contact.username }}

        </a>

    </li>
{% endfor %}

